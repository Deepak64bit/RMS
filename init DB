CREATE DATABASE RESTAURANT;
USE RESTAURANT;
CREATE TABLE EMPLOYEE (ID int, name varchar(50), address varchar (100), age int, salary int, phonenumber bigint);
CREATE TABLE LOGIN ( username varchar(50), password varchar(50));
INSERT INTO LOGIN values('RestOwn', 'Owner1234');
CREATE TABLE MENU (token int, name varchar(50),price int,time int);
CREATE TABLE OWNERORDERLIST(price int, tablenum int,date varchar(19),type varchar(8));
CREATE TABLE ORDERSLIST(tablenum int, name varchar(50),date varchar(19), type varchar(8));
CREATE TABLE CHIEFORDERLIST(name varchar(50),tablenum int);
CREATE TABLE WAITERS(waiter int, status int); 
CREATE TABLE TABLES (number int , status int);
CREATE TABLE WAITERORDERS (waiter int , orders int);
